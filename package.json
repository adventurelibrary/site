{
  "name": "nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev-admin": "env ADMIN=1 nuxt",
    "dev-admin-local": "env ADMIN=1 ADVL_BASE_URL=http://localhost:4300 nuxt",
    "build": "nuxt build",
    "build2": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "eslint --ext .ts,.js,.vue ."
  },
  "dependencies": {
    "@coreui/coreui": "^4.0.0-alpha.3",
    "@coreui/vue": "^3.2.9",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/dotenv": "^1.4.1",
    "amazon-cognito-identity-js": "^4.6.0",
    "aws-amplify": "^3.3.27",
    "aws-sdk": "^2.879.0",
    "core-js": "^3.6.5",
    "http-status-codes": "^2.1.4",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.1.2",
    "nuxt": "^2.14.6",
    "nuxt-property-decorator": "^2.9.1",
    "sass": "^1.32.11",
    "vue-fragment": "^1.5.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.12.17",
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@types/js-cookie": "^2.2.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
